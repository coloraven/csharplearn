<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <Company>FZDD</Company>
    <Copyright>jjj</Copyright>
    <Description>ddd</Description>
    <FileVersion>lsdi</FileVersion>
    <SelfContained>true</SelfContained>
    <!-- 指定目标框架 -->
    <TargetFramework>net7.0</TargetFramework>

    <!-- 添加运行时标识符，如：win-x64, linux-x64, osx-x64 -->
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>

    <!-- 设置为自包含 -->
    <SelfContained>true</SelfContained>

    <!-- 发布为单文件 -->
    <PublishSingleFile>true</PublishSingleFile>

    <!-- (可选) 如果您想减少发布文件的大小，可以启用就地修剪 -->
    <!-- <PublishTrimmed>true</PublishTrimmed> -->

    <!-- (可选) 设置为true以启用就地修剪的更激进模式 -->
    <!-- <PublishReadyToRun>true</PublishReadyToRun> -->

    <!-- (可选) 启用发布单一文件时的压缩 -->
    <!-- <PublishSingleFileCompression>true</PublishSingleFileCompression> -->
  
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="RestSharp" Version="110.2.0" />
  </ItemGroup>

</Project>
